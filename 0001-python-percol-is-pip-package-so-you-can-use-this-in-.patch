From b3a275d84ec57d7f1d2c07298d3e067ad5d8e447 Mon Sep 17 00:00:00 2001
From: Kei Okada <kei.okada@gmail.com>
Date: Wed, 4 Feb 2015 08:53:28 +0900
Subject: [PATCH] python-percol is pip package so you can use this in deb
 package

---
 package.xml |   18 ++++++++++++++++--
 1 file changed, 16 insertions(+), 2 deletions(-)

diff --git a/package.xml b/package.xml
index 69b3027..81adb33 100644
--- a/package.xml
+++ b/package.xml
@@ -17,8 +17,22 @@
 
   <build_depend>git</build_depend>
   <build_depend>rosgraph_msgs</build_depend>
-  <run_depend>python-percol</run_depend>
-  <run_depend>python-colorama</run_depend>
+  <!--
+This release uses a dependency on python-percol in the package
+https://github.com/jsk-ros-pkg/jsk_common/blob/02b0fd487b1fa5cef967a2a875acf8564004bea2/jsk_tools/package.xml
+
+As @wjwwood pointed out when you added the rosdep key (#6993
+(comment)) you can not depend on packages which are not available from
+Debian packages in your releases packages:
+http://jenkins.ros.org/view/IbinT64/job/ros-indigo-multi-map-server_binarydeb_trusty_amd64/15/console
+      <run_depend>python-percol</run_depend>
+  -->
+  <!--
+  python-colorama is only avilable after trusty 
+http://packages.ubuntu.com/search?searchon=names&keywords=python-colorama
+https://github.com/ros/rosdistro/pull/7273
+      <run_depend>python-colorama</run_depend>
+  -->
   <run_depend>rosgraph_msgs</run_depend>
   <run_depend>rospy</run_depend>
   <run_depend>rqt_reconfigure</run_depend>
-- 
1.7.9.5

