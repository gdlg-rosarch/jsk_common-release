+def install_apt_packages(target_dir):
+    env = ChrootEnvironment(target_dir)
+    with env:
+        logger = logging.getLogger("pxe")
+        logger.info("installing base apt packages")
+        chroot_command(target_dir, ["apt-get", "update"])
