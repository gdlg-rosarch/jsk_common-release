+  bool SeriesBoolean::getValue()
+  {
+    if (buf_.size() == 0) {
+      return false;
+    }
+    else {
+      for (boost::circular_buffer<bool>::iterator it = buf_.begin();
+           it != buf_.end();
+           ++it) {
+        if (!*it) {
+          return false;
+        }
+      }
+      return true;
+    }
+  }
