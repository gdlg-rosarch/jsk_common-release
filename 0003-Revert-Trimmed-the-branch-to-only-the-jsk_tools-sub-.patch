+    [[ $LSB_RELEASE =~ (lucid|maverick) ]] ) || \
+   ( [ "$DISTRIBUTION" == "diamondback" ] && \
+    [[ $LSB_RELEASE =~ (lucid|maverick|natty) ]] ) || \
+   ( [ "$DISTRIBUTION" == "diamondback" ] && \
+    [[ $LSB_RELEASE =~ (lucid|maverick|natty) ]] ) || \
+   ( [ "$DISTRIBUTION" == "electric" ] && \
+    [[ $LSB_RELEASE =~ (lucid|maverick|natty|oneiric) ]] ) || \
+   ( [ "$DISTRIBUTION" == "fuerte" ] && \
