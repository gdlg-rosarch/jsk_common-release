+OPT=`getopt -o hipeynsr: -l help,show-install,show-package,show-environment,yes,rtm,start-jsk,from-source,from-full-source,use-shadow-fixed,no-sudo,repositories -- $*`
+if [ $? != 0 ]; then
+    usage
+fi
+DEBIAN_REPOSITORY='http://packages.ros.org/ros/ubuntu'
+SUDO=sudo
