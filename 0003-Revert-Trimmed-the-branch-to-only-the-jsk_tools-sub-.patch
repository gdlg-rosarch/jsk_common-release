+  cv::Point ImageView2::ratioPoint(double x, double y)
+  {
+    if (last_msg_) {
+      return cv::Point(last_msg_->width * x,
+                       last_msg_->height * y);
+    }
+    else {
+      return cv::Point(0, 0);
+    }
+  }
